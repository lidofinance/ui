{
  "name": "@lidofinance/lido-ui",
  "version": "4.0.0-canary",
  "homepage": "https://github.com/lidofinance/ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lidofinance/ui.git"
  },
  "bugs": {
    "url": "https://github.com/lidofinance/ui/issues"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./index.css": {
      "import": "./dist/esm/index.css",
      "require": "./dist/cjs/index.css"
    },
    "./styles/*": "./dist/styles/*"
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "storybook dev -p 5555",
    "build-storybook": "yarn build-storybook:prebuild && storybook build",
    "build-storybook:prebuild": "yarn css-variables:generate",
    "build": "zx ./scripts/build.cjs",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:coverage": "node --experimental-vm-modules ./node_modules/.bin/jest coverage",
    "lint": "set +e; yarn lint:js; yarn lint:css; set -e",
    "lint:fix": "set +e; yarn lint:js:fix; yarn lint:css:fix; set -e",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "lint:css": "stylelint '**/*.css'",
    "lint:css:fix": "yarn lint:css --fix",
    "types": "tsc --noEmit",
    "prepare": "husky",
    "release": "semantic-release",
    "release:dry": "yarn release --dry-run",
    "icons:convert": "node ./packages/icons/converter/index.cjs",
    "css-variables:generate": "node ./scripts/generate-css-variables.mjs"
  },
  "lint-staged": {
    "*.{ts,tsx,js,cjs,mjs}": "eslint --fix",
    "*.css": "stylelint --fix"
  },
  "peerDependencies": {
    "next": ">=12.0.0 <15.0.0",
    "react": "16 || 17 || 18",
    "react-dom": "16 || 17 || 18",
    "react-is": "16 || 17 || 18"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "28.0.3",
    "classnames": "2.5.1",
    "postcss-mixins": "11.0.3",
    "react-collapsed": "4.2.0",
    "react-jazzicon": "1.0.4",
    "react-toastify": "11.0.5",
    "react-transition-group": "4",
    "rollup-plugin-copy": "3.5.0",
    "ua-parser-js": "2.0.3",
    "use-callback-ref": "1.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.27.0",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@csstools/postcss-global-data": "3.0.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@semrel-extra/npm": "1.2.2",
    "@storybook/addon-actions": "8.6.12",
    "@storybook/addon-docs": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/cli": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-webpack5": "8.6.12",
    "@storybook/storybook-deployer": "2.8.16",
    "@storybook/test": "8.6.12",
    "@storybook/theming": "8.6.12",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-prettier": "8.1.0",
    "@svgr/plugin-svgo": "5.5.0",
    "@testing-library/react": "15.0.7",
    "@types/jest": "29.5.14",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-is": "17.0.2",
    "@types/react-transition-group": "4.4.12",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.21",
    "browserslist": "4.24.5",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "0.12.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-haste-map": "29.7.0",
    "jest-resolve": "29.7.0",
    "jest-runner": "29.7.0",
    "lint-staged": "15.5.1",
    "next": "14.2.28",
    "postcss": "8.5.3",
    "postcss-custom-media": "11.0.5",
    "postcss-import": "16.1.0",
    "postcss-loader": "8.1.1",
    "postcss-nested": "7.0.2",
    "prettier": "3.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rimraf": "6.0.1",
    "rollup": "3.29.5",
    "rollup-plugin-postcss": "4.0.2",
    "semantic-release": "19.0.5",
    "storybook": "8.6.12",
    "stylelint": "15.11.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-prettier": "4.1.0",
    "ts-jest": "29.3.4",
    "tslib": "2.3.1",
    "typescript": "5.8.3",
    "zx": "8.5.3"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "packageManager": "yarn@4.9.0"
}
