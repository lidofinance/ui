# Check if any SVG files are staged and run icon generation
STAGED_SVG_FILES=$(git diff --cached --name-only | grep '\.svg$' || true)
if [ -n "$STAGED_SVG_FILES" ]; then
  echo "SVG files changed, running yarn icons:convert..."
  yarn icons:convert
  git add packages/icons/components/ packages/icons/index.tsx
fi

npx lint-staged
